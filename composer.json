{
    "name": "bordieris/nova_extended_place",
    "description": "A Laravel Nova Extended Place field. Allows to discover from an address or place a marker on map to get coordinates and reverse geocoding the coordinates to get a street address",
    "keywords": [
        "laravel",
        "nova",
        "geocoding",
        "address",
        "geo",
        "map"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Bordieris\\ExtendedPlace\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bordieris\\ExtendedPlace\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
